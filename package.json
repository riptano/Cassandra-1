{
	"name": "DSE Graph",
	"version": "0.1.1",
	"versionDate": "2019-08-01",
	"author": "Datastax",
	"engines": {
		"hackolade": "3.4.0",
		"hackoladePlugin": "1.2.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "CASSANDRA",
			"title": "DSE Graph",
			"versions": [
				"DSE 4.x",
				"DSE 5.x",
				"DSE 6.X"
			]
		},
		"features": {
			"nestedCollections": false,
			"disablePatternField": false,
			"enableForwardEngineering": true,
			"disableMultipleTypes": true,
			"enableReverseEngineering": true,
			"disableChoices": true,
			"enableJsonType": true,
			"enableArrayItemName": true
		}
	},
	"description": "Hackolade plugin for DSE Graph"
}